<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.wnagzihxain.teacherdemo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".ControlActivity"></activity>

        <activity android:name=".SignActivity"></activity>

        <activity android:name=".ManageStuInfoActivity"></activity>

        <activity android:name=".ManageStuInfoDetail"></activity>

        <activity android:name=".VacateActivity"></activity>

        <activity android:name=".VacateDetail"></activity>
    </application>

</manifest>